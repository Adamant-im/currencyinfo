    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.0.0",
    "adamant-api": "^2.3.0",
    "axios": "^1.6.7",
    "chalk": "4",
    "ioredis": "^5.3.2",
    "json5": "^2.2.3",
    "mongoose": "^8.1.2",
    "ms": "^2.1.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/schematics": "^10.0.0",
